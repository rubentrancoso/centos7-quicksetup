export PROXY_USERNAME=""
export PROXY_PASSWORD=""
export PROXY_HOST=""
export PROXY_PORT=""
export PROXY_EXCEPTIONS=""

export PROXY_URL_AUTH="$PROXY_USERNAME:$PROXY_PASSWORD@"
export HTTP_PROXY="http://$PROXY_USERNAME:$PROXY_PASSWORD@$PROXY_HOST:$PROXY_PORT"
export HTTPS_PROXY="htts://$PROXY_USERNAME:$PROXY_PASSWORD@$PROXY_HOST:$PROXY_PORT"
export PROXY_SERVER="http://$PROXY_HOST:$PROXY_PORT"
export PROXY_SERVER_WITHPASS="http://$PROXY_URL_AUTH""$PROXY_HOST:$PROXY_PORT"
